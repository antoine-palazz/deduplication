   ],
   "source": [
    "for i in tqdm(range(n_ads)):\n",
    "    for j in range(i+1,n_ads):\n",
    "        if (data.iloc[i, 1] == data.iloc[j, 1]) and (data.iloc[i, 2] == data.iloc[j, 2]):\n",
    "            duplicates.append({'id1': i+1, 'id2': j+1, 'type': 'FULL'})\n",
    "\n",
    "len(duplicates)"
